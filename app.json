{
  "expo": {
    "name": "Display Antrian TV",
    "slug": "antrian-webview-display",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "WAKE_LOCK",
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "WAKE_LOCK"
      ],
      "package": "com.antrian.display.tv",
      "versionCode": 1,
      "screenOrientation": "landscape",
      "allowBackup": false,
      "hardwareAccelerated": true,
      "keepScreenOn": true,
      "usesCleartextTraffic": true
    },
    "extra": {
      "eas": {
        "projectId": "8bffdb2d-d88c-4482-8098-cbff194e56ed"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/8bffdb2d-d88c-4482-8098-cbff194e56ed"
    },
    "ios": {},
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true,
            "enableProguardInReleaseBuilds": false,
            "enableShrinkResourcesInReleaseBuilds": false
          }
        }
      ]
    ]
  }
}
